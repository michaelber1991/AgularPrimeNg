{
	"useTabs": true,
	"tabWidth": 2,
	"printWidth": 100,
	"singleQuote": true,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"semi": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"endOfLine": "lf",

	"organizeAttributes": {
		"attributesPerLine": 1
	}
}
