{
	"name": "AppName",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"serve:ssr:AppName": "node dist/AppName/server/server.mjs",
		"lint": "ng lint",
		"prepare": "husky install",
		"format": "prettier --write \"./src/**/*.{ts,json,html,scss}\"",
		"lint-format": "npm run format &&  eslint --fix",
		"storybook": "ng run AppName:storybook",
		"build-storybook": "ng run AppName:build-storybook"
	},
	"lint-staged": {
		"*": "prettier --write \"./src/**/*.{ts,json,html,scss}\"",
		"src/**/*.ts": "eslint --fix"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^19.0.5",
		"@angular/cdk": "^19.0.4",
		"@angular/common": "^19.0.5",
		"@angular/compiler": "^19.0.5",
		"@angular/core": "^19.0.5",
		"@angular/forms": "^19.0.5",
		"@angular/platform-browser": "^19.0.5",
		"@angular/platform-browser-dynamic": "^19.0.5",
		"@angular/platform-server": "^19.0.5",
		"@angular/router": "^19.0.5",
		"@angular/ssr": "^19.0.6",
		"@ngx-translate/core": "^15.0.0",
		"@ngx-translate/http-loader": "^8.0.0",
		"express": "^4.18.2",
		"keycloak-angular": "^19.0.2",
		"primeicons": "^7.0.0",
		"primeng": "19.0.2",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.15.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^19.0.6",
		"@angular-eslint/builder": "17.3.0",
		"@angular-eslint/eslint-plugin": "17.3.0",
		"@angular-eslint/eslint-plugin-template": "17.3.0",
		"@angular-eslint/schematics": "18.4.0",
		"@angular-eslint/template-parser": "17.3.0",
		"@angular/cli": "^19.0.6",
		"@angular/compiler-cli": "^19.0.5",
		"@chromatic-com/storybook": "^1.7.0",
		"@compodoc/compodoc": "^1.1.25",
		"@storybook/addon-docs": "^8.2.9",
		"@storybook/addon-essentials": "^8.2.9",
		"@storybook/addon-interactions": "^8.2.9",
		"@storybook/addon-links": "^8.2.9",
		"@storybook/addon-onboarding": "^8.2.9",
		"@storybook/angular": "^8.4.7",
		"@storybook/blocks": "^8.2.9",
		"@storybook/test": "^8.2.9",
		"@types/express": "^4.17.17",
		"@types/jasmine": "~5.1.0",
		"@types/node": "^18.18.0",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.0.0-alpha.1",
		"eslint-plugin-storybook": "^0.8.0",
		"eslint-plugin-unused-imports": "^4.0.1",
		"husky": "^8.0.0",
